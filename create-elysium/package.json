{
	"name": "create-elysium",
	"version": "0.1.8",
	"license": "Apache-2.0",
	"description": "Bootstrap you next Elysium.js app!",
	"author": {
		"name": "Axel Nana",
		"email": "axel.nana@workbud.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Workerly/elysium"
	},
	"homepage": "https://github.com/Workerly/elysium",
	"scripts": {
		"format": "prettier --write \"./**/*.ts\"",
		"build": "bun build index.ts --outfile=index.js --target bun --minify"
	},
	"keywords": [
		"elysium",
		"framework",
		"web",
		"api",
		"typescript"
	],
	"bin": {
		"create-elysium": "./index.js"
	},
	"files": [
		"index.js",
		"template-*/**",
		"feature-*/**",
		"shared-*/**",
		"plugin-*/**"
	],
	"type": "module",
	"devDependencies": {
		"@types/prompts": "^2.4.9"
	},
	"dependencies": {
		"prompts": "^2.4.2",
		"radash": "catalog:"
	}
}
